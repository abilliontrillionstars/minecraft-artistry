{
  "json.schemas": [
    {
      "fileMatch": ["**/avatar.json"],
      "url": ".vscode/avatar.schema.json"
    }
  ],
  "[lua]": {
    "editor.suggest.showDeprecated": false
  },
  "Lua.diagnostics.globals": [
    "action_wheel",
    "vectors",
    "models",
    "animations",
    "pings",
    "host",
    "events",
    "vec",
    "textures",
    "client",
    "vanilla_model",
    "player",
    "keybinds",
    "avatar",
    "sounds",
    "world",
    "renderer"
  ],
  "Lua.diagnostics.disable": [
    "duplicate-set-field"
  ]
}
